{
  "name": "flashmind-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "migrate": "node-pg-migrate up --envPath .env",
    "migrate:create": "node-pg-migrate create --envPath .env"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@google/genai": "latest",
    "pg": "^8.11.3",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4",
    "node-pg-migrate": "^6.2.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "google-auth-library": "^9.6.3",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.0.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "ts-node-dev": "^2.0.0"
  }
}